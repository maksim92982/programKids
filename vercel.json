{
  "version": 2,
  "buildCommand": "echo 'Static site with serverless api'",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/modules.json",
      "dest": "/modules.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
